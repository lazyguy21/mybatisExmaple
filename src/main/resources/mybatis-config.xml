<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties resource="jdbc.properties"></properties>
    <typeAliases>
        <!--<typeAlias alias="Student" type="org.yyf.mybatisexmaple.domain.Student"/>-->
        <package name="org.yyf.mybatisexmaple.domain"/>
    </typeAliases>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="${dataSource.driverClassName}"/>
                <property name="url" value="${dataSource.url}"/>
                <property name="username" value="${dataSource.username}"/>
                <property name="password" value="${dataSource.password}"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="org/yyf/mybatisexmaple/mappers/StudentMapper.xml"/>
        <mapper resource="org/yyf/mybatisexmaple/mappers/EnumTestDomainMapper.xml"/>
        <!--<package name="org.yyf.mybatisexmaple.mappers"></package>-->
    </mappers>
</configuration>